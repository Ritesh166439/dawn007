{% if pagination.pages > 0 %}
<nav role="navigation" class="flex justify-center gap-1 font-medium">
    {% if pagination.previouse %}
    <a href="{{ pagination.previouse.url }}"
        class="font-medium text-gray-600 border border-gray-600 hover:text-gray-700">{{
        pagination.previouse.title }}</a>
    {% endif %}
    {% if pagination.next %}
    <a href="{{ pagination.previous.url }}"
        class="font-medium text-gray-600 border border-gray-600 hover:text-gray-700">{{ pagination.previous.title }}</a>
    <a href="{{ pagination.next.url }}" class="font-medium text-gray-600 border border-gray-600 hover:text-gray-700">{{
        pagination.next.title }}</a>
    {% endif %}
</nav>
{% endif %}